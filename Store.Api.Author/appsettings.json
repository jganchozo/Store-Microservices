{
  "ConnectionStrings": {
    "psqldbconnection": "Server={server};Port={port};Database={databasename};User Id={user};Password={password};"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
